INTEGER(KIND=JPIM) ,OPTIONAL, INTENT(IN)  :: KRESOL
EXTERNAL  FSPGL_PROC
OPTIONAL  FSPGL_PROC
REAL(KIND=JPRM),OPTIONAL    ,INTENT(IN)  :: PGP(:,:,:)
REAL(KIND=JPRM),OPTIONAL    ,INTENT(IN)  :: PGPUV(:,:,:,:)
